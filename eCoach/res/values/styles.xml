<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="style_common_titlebar">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/common_titlebar_height</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:background">@color/color_titlebar_bkg</item>
    </style>
    <style name="style_common_titletv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:textColor">@color/color_titlebar_txt</item>
        <item name="android:textSize">@dimen/txtsize_xlarge</item>
    </style>
    <style name="style_common_title_leftbtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">3dp</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_marginLeft">@dimen/common_titlebar_horizontal_margin</item>
    </style>
    <style name="style_common_title_backbtn" parent="@style/style_common_title_leftbtn">
        <item name="android:src">@drawable/icon_navi_back</item>
        <item name="android:onClick">onBackButtonClicked</item>
    </style>
    
     <style name="style_common_title_rightbtn">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:padding">2dp</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_marginRight">@dimen/common_titlebar_horizontal_margin</item>
    </style>
    
    <style name="style_common_red_dot">
        <item name="android:layout_width">@dimen/common_reddot_width</item>
        <item name="android:layout_height">@dimen/common_reddot_height</item>
        <item name="android:background">@drawable/bkg_common_reddot</item>
    </style>
    <style name="style_texture_red_dot" parent="style_common_red_dot">
        <item name="android:textColor">@android:color/white</item>
        <item name="android:textSize">@dimen/txtsize_micro</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="style_splash_viewpager_dot">
        <item name="android:layout_width">@dimen/common_reddot_width</item>
        <item name="android:layout_height">@dimen/common_reddot_height</item>
        <item name="android:background">@drawable/bkg_splash_dots</item>
        <item name="android:layout_marginLeft">@dimen/padding_micro</item>
        <item name="android:layout_marginRight">@dimen/padding_micro</item>
    </style>
    
    
    <style name="style_pcenter_scoreprg">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/list_pcenter_scoreup</item>
        <item name="android:layout_width">250dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">22dp</item>  
        <item name="android:maxHeight">22dp</item>
        <item name="android:max">100</item>
    </style>
    <style name="style_pcenter_menu_itemtv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginTop">2dp</item>
        <item name="android:layout_marginLeft">30dp</item>
        <item name="android:layout_marginRight">30dp</item>
        <item name="android:background">@drawable/bkg_personal_center_menu</item>
        <item name="android:textColor">@color/color_pcenter_textcolor</item>
        <item name="android:textSize">@dimen/txtsize_default</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingBottom">5dp</item>
        <item name="android:paddingLeft">20dp</item>
        <item name="android:paddingRight">20dp</item>
        <item name="android:drawablePadding">10dp</item>
        <item name="android:focusable">true</item>
        <item name="android:clickable">true</item>
    </style>
    <style name="style_pcenter_stat_linear">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingLeft">10dp</item>
    </style>
    <style name="style_pcenter_stattv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:paddingTop">4dp</item>
        <item name="android:paddingBottom">4dp</item>
        <item name="android:textColor">@color/color_pcenter_textcolor</item>
        <item name="android:textSize">@dimen/txtsize_default</item>
        <item name="android:gravity">center_horizontal</item>
    </style>
    
    <style name="style_coach_ratingbar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/list_coach_ratingbar</item>  
        <item name="android:minHeight">30dp</item>  
        <item name="android:maxHeight">30dp</item>  
    </style>
    
    <style name="style_main_ordernum_progress">
        <!-- this attribute make the progress bar to be horizontal. -->
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/list_main_order</item>
        <item name="android:layout_width">180dp</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:minHeight">20dp</item>  
        <item name="android:maxHeight">20dp</item>
        <item name="android:max">100</item>
    </style>
    
    <style name="style_main_ordernum_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_main_text_normal</item>
        <item name="android:textSize">@dimen/txtsize_default</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">@dimen/main_ordernumtv_padding_horizontal</item>
        <item name="android:paddingRight">@dimen/main_ordernumtv_padding_horizontal</item>
    </style>
    <style name="style_main_bottom_menutv">
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color_theme_orange</item>
        <item name="android:textSize">@dimen/txtsize_large</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
        <item name="android:padding">5dp</item>
        <item name="android:drawablePadding">5dp</item>
    </style>
    
    <style name="style_login_form">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/margin_xlarge</item>
        <item name="android:layout_marginRight">@dimen/margin_xlarge</item>
        <item name="android:padding">8dp</item>
        <item name="android:textSize">@dimen/login_item_textsize</item>
        <item name="android:layout_marginTop">9dp</item>
        <item name="android:layout_marginBottom">9dp</item>
    </style>
    
    <style name="style_register_steptv">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@drawable/sel_register_steptv_txtcolor</item>
        <item name="android:textSize">@dimen/txtsize_xlarge</item>
        <item name="android:paddingTop">@dimen/padding_middle</item>
        <item name="android:paddingBottom">@dimen/padding_middle</item>
        <item name="android:paddingLeft">@dimen/padding_micro</item>
        <item name="android:paddingRight">@dimen/padding_micro</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bkg_register_steptv</item>
    </style>
    
    <style name="style_register_fragment_root">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/color_all_bkg</item>
        <item name="android:paddingLeft">@dimen/padding_large</item>
        <item name="android:paddingRight">@dimen/padding_large</item>
        <item name="android:paddingTop">@dimen/padding_large</item>
    </style>
    
    <style name="style_register_fragbtn_next">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_large</item>
        <item name="android:background">@drawable/bkg_register_btn_next</item>
        <item name="android:padding">@dimen/padding_middle</item>
        <item name="android:text">@string/str_register_step_next</item>
        <item name="android:textColor">@drawable/sel_register_btn_next_txtcolor</item>
        <item name="android:textSize">@dimen/txtsize_large</item>
        <item name="android:enabled">false</item>
    </style>
    
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

</resources>